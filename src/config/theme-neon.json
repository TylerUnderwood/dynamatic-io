{
  "color": {
    "azure": "#0080ff",
    "royal-purple": "#c000ff",
    "fuchsia": "#ff0080",
    "starburst": "#ff0040",
    "lava-orange": "#ff4000",
    "neon-yellow": "#c0ff00",
    "jade": "#00ff80",
    "sea-foam": "#00ffc0",
    "grey": "#808080",
    "dark": {
      "100": "#282a30",
      "200": "#202228",
      "400": "#181a20",
      "600": "#101218",
      "800": "#080a10",
      "DEFAULT": "var(--dark-100)"
    },
    "light": {
      "100": "#ffffff",
      "200": "#f8f8f8",
      "400": "#eeeeee",
      "600": "#e8e8e8",
      "800": "#dddddd",
      "DEFAULT": "var(--light-200)"
    }
  },
  "light": {
    "primary": "var(--azure)",
    "secondary": "var(--royal-purple)",
    "accent": "var(--jade)",
    "input": "var(--azure)",
    "marker": "var(--jade)",
    "muted": "var(--grey)",
    "base": "var(--light-100)",
    "offset": "var(--light-200)",
    "contrast": "var(--light-400)",
    "head": "var(--dark-100)",
    "face": "var(--dark-200)",
    "body": "var(--dark-400)"
  },
  "dark": {
    "primary": "var(--azure)",
    "secondary": "var(--royal-purple)",
    "accent": "var(--jade)",
    "input": "var(--azure)",
    "marker": "var(--jade)",
    "muted": "var(--grey)",
    "base": "var(--dark-100)",
    "offset": "var(--dark-200)",
    "contrast": "var(--dark-400)",
    "head": "var(--light-100)",
    "face": "var(--light-200)",
    "body": "var(--light-400)"
  },
  "default": {
    "primary": "var(--dark-primary)",
    "secondary": "var(--dark-secondary)",
    "accent": "var(--dark-accent)",
    "input": "var(--dark-input)",
    "marker": "var(--dark-marker)",
    "muted": "var(--dark-muted)",
    "base": "var(--dark-base)",
    "offset": "var(--dark-offset)",
    "contrast": "var(--dark-contrast)",
    "head": "var(--dark-head)",
    "face": "var(--dark-face)",
    "body": "var(--dark-body)"
  },
  "markup": {
    "marker": {
      "color": "var(--theme-marker)"
    },
    "font": {
      "head": "'Roboto Condensed', sans-serif",
      "face": "'Roboto', sans-serif",
      "meta": "'Roboto Mono', sans-serif"
    },
    "break-size": "1.6rem",
    "tab-size": "2.0rem"
  },
  "input": {
    "color": "var(--theme-input)",
    "hover": "var(--theme-accent)",
    "active": "var(--theme-secondary)",
    "height": {
      "large": "3.0rem",
      "DEFAULT": "2.4rem",
      "small": "1.6rem"
    },
    "border": {
      "width": {
        "large": "4px",
        "DEFAULT": "2px",
        "small": "1px"
      }
    },
    "pad": {
      "large": "1.2rem",
      "DEFAULT": "0.8rem",
      "small": "0.5rem"
    },
    "round": "1px",
    "timing": "200ms",
    "easing": "ease"
  },
  "screen": {
    "sm": "480px",
    "md": "768px",
    "lg": "1024px",
    "xl": "2560px"
  },
  "max-w": {
    "copy": "36rem",
    "page": "48rem",
    "site": "64rem"
  },
  "size": {
    "1": "1px",
    "2": "2px",
    "4": "4px",
    "8": "0.4rem",
    "10": "0.5rem",
    "12": "0.6rem",
    "14": "0.7rem",
    "16": "0.8rem",
    "20": "1.0rem",
    "24": "1.2rem",
    "32": "1.6rem",
    "48": "2.4rem",
    "64": "3.2rem",
    "80": "4.0rem",
    "96": "4.8rem",
    "128": "6.4rem",
    "break": "var(--break-size)",
    "tab": "var(--tab-size)",
    "DEFAULT": "1em",
    "rem": "1rem"
  },
  "import": {
    "fonts": "https://fonts.googleapis.com/css2?family=Roboto+Condensed:wght@300;700&family=Roboto+Mono:wght@300;500;700&family=Roboto:wght@100;300;500;700;900&display=swap"
  }
}
